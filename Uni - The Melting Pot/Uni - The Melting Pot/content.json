{
    "Format": "2.0.0",

    "DynamicTokens": 
    [
        {
            "Name": "TMPHaramTag",
            "Value": "food_haram",
            "When": 
            {
                "HasMod |contains=Aimon111.SoyBeanMeats": false
            }
        },

        {
            "Name": "TMPTeryfTag",
            "Value": "food_tref",
            "When": 
            {
                "HasMod |contains=Aimon111.SoyBeanMeats": false
            }
        },

        {
            "Name": "TMPHaramTag",
            "Value": "food_halal",
            "When": 
            {
                "HasMod |contains=Aimon111.SoyBeanMeats": true
            }
        },

        {
            "Name": "TMPTeryfTag",
            "Value": "food_kosher",
            "When": 
            {
                "HasMod |contains=Aimon111.SoyBeanMeats": true
            }
        }
    ],
   
    "Changes": 
