{
  "Format": "2.5.0",
  "ConfigSchema": {
    "EventLookupHotkey": {
      "AllowBlank": true,
      "Default": "N"
    },
    "ResetTerrainFeaturesHotkey": {
      "AllowBlank": true,
      "Default": "L"
    },
    "ScreenshotHotkey": {
      "AllowBlank": true,
      "Default": "F9"
    }
  },
  "Changes": [
    {
      "Action": "Include",
      "FromFile": "data/EventLookup.json",
      "When": {
        "HasMod": "shekurika.EventLookup"
      }
    },
    {
      "Action": "Include",
      "FromFile": "data/FashionSense.json",
      "When": {
        "HasMod": "PeacefulEnd.FashionSense"
      }
    },
    {
      "Action": "Include",
      "FromFile": "data/InstantBuildings.json",
      "When": {
        "HasMod": "BitwiseJonMods.InstantBuildings"
      }
    },
    {
      "Action": "Include",
      "FromFile": "data/ResetTerrainFeatures.json",
      "When": {
        "HasMod": "MabelSyrup.ResetTerrainFeatures"
      }
    },
    {
      "Action": "Include",
      "FromFile": "data/Screenshot.json",
      "When": {
        "HasMod": "aedenthorn.Screenshot"
      }
    }
  ],
  "$schema": "https://smapi.io/schemas/content-patcher.json"
}