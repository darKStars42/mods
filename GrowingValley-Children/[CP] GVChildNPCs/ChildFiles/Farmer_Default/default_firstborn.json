{
   "Changes": [
   
        {
			"LogName": "Trigger action to add mail flag for tracking loaded default firstborn.",
            "Action": "EditData",
            "Target": "Data/TriggerActions",
            "Entries": {
                "{{ModId}}_Add_Default_Firstborn": {
                    "Id": "{{ModId}}_Add_Default_Firstborn",
                    "Trigger": "DayStarted siliconmodding.GVChildrenSMAPI_OnConfigChangedLate",
                    "Action": "AddMail Host {{ModId}}_Default_Firstborn Received",
					"MarkActionApplied": false,
                },
                "{{ModId}}_Add_GrowingValley_Firstborn_{{FirstbornGender}}": {
                    "Id": "{{ModId}}_Add_GrowingValley_Firstborn_{{FirstbornGender}}",
                    "Trigger": "DayStarted siliconmodding.GVChildrenSMAPI_OnConfigChangedLate",
                    "Action": "AddMail Current GrowingValley_Firstborn_{{FirstbornGender}} Received",
					"MarkActionApplied": false,
                },
			}
		},
   
