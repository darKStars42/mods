	//Ideally, don't touch this. It ensures that the main Growing Valley mod doesn't load a kid over yours.
        {
			"LogName": "Trigger action to add mail flag for tracking a custom child.",
            "Action": "EditData",
            "Target": "Data/TriggerActions",
            "Entries": {
                "{{ModId}}_Add_GrowingValley_Custom_Firstborn": {
                    "Id": "{{ModId}}_Add_GrowingValley_Custom_Firstborn",
                    "Trigger": "DayStarted siliconmodding.GVChildrenSMAPI_OnConfigChangedLate",
                    "Action": "AddMail Current GrowingValley_Custom_Firstborn Received",
					"MarkActionApplied": false,
                },
			}
		},
		
	//Keep this as-is if this child is meant to be male. If the child is actually meant to be female (and has "male" as their internal gender, such as in cases where you want siblings with the same gender), you can change all 3 instances here of "Male" to "Female"
