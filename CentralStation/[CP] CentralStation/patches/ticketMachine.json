/*

    This file adds the Central Station ticket machine which is patched into other maps as needed.

*/
{
    "$schema": "https://smapi.io/schemas/content-patcher.json",

    "Changes": [
        {
            "Action": "Load",
            "Target": "Mods/{{MainModId}}/TicketMachine",
            "FromFile": "assets/maps/ticketMachine.tmx"
        }
    ]
}
