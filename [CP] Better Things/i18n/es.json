  "dinosaur_shirt.name": "Camisa de Dinosaurio",
  "dinosaur_shirt.description": "Una Camisa de Dinosaurio.",
  "dinosaur_alt.shirt.name": "Camisa de Dinosaurio Alternativa",
  "dinosaur_alt.shirt.description": "Una Camisa de Dinosaurio Alternativa.",
  
  "dinosaur_boot.name": "Botas de Dinosaurio",
  "dinosaur_boot.description": "Unas Botas de Dinosaurio.",
  "dinosaur_red_boot.name": "Botas de Dinosaurio Rojas",
