    {
      "Action": "Include",
      "FromFile": "bookshelf.json",
      "LogName": "Add Bookshelf Books"
    },
    {
      "Action": "Include",
      "FromFile": "newlocations.json",
      "LogName": "Add New Locations"
    },
    {
      "Action": "Include",
      "FromFile": "cjbwarps.json",
      "LogName": "Add CJB Warps",
      "When": {
        "HasMod": "CJBok.CheatsMenu"
      }
    },
    {
      "Action": "Include",
      "FromFile": "shops.json",
      "LogName": "Add Shops"
    },
    {
      "Action": "Include",
      "FromFile": "destinations.json",
      "LogName": "Add Stops"
    },
    {
      "Action": "Include",
      "FromFile": "stringsfrommaps.json",
      "LogName": "Add Strings from Maps"
    }
  ],
