    {
      "LogName": "Include dialogue changes because vanilla tank blocked",
      "Action": "Include",
      "FromFile": "data/dialogues.json",
      "When": {
        "HasMod": "camiska.FrogTankFrogEgg",
        "Query: ( ( '{{HasMod |contains=shaklin.SebastianFrogTankSwitch}}' = 'true' AND '{{camiska.FrogTankFrogEgg/SebSwitchBlocksFrogTank}}' = 'true' ) OR ( '{{HasMod |contains=EvhanCorinthi.SRR}}' = 'true' AND '{{camiska.FrogTankFrogEgg/SRRHideSanctuary}}' = 'true' ) OR ( '{{camiska.FrogTankFrogEgg/EnableSebMapTanks}}' = 'false' ) OR ( '{{camiska.FrogTankFrogEgg/HideVanillaSebTank}}' = 'true' ) )": true
      }
    },
    {
      "LogName": "Include event changes because vanilla tank blocked",
      "Action": "Include",
      "FromFile": "data/events.json",
      "When": {
        "HasMod": "camiska.FrogTankFrogEgg",
        "Query: ( ( '{{HasMod |contains=shaklin.SebastianFrogTankSwitch}}' = 'true' AND '{{camiska.FrogTankFrogEgg/SebSwitchBlocksFrogTank}}' = 'true' ) OR ( '{{HasMod |contains=EvhanCorinthi.SRR}}' = 'true' AND '{{camiska.FrogTankFrogEgg/SRRHideSanctuary}}' = 'true' ) OR ( '{{camiska.FrogTankFrogEgg/EnableSebMapTanks}}' = 'false' ) OR ( '{{camiska.FrogTankFrogEgg/HideVanillaSebTank}}' = 'true' ) )": true
      }
    },

    { "Action": "Load", "Target": "Mods/{{ModId}}/AquariumTadpole",     "FromFile": "assets/AquariumTadpole.png" },
    { "Action": "Load", "Target": "Mods/{{ModId}}/Tadpole",              "FromFile": "assets/Tadpole.png" },
