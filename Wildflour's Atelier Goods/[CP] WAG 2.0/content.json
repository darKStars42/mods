    "Changes": [

//Include Objects

        {
            "LogName": "WAG Objects",
            "Action": "Include",
            "FromFile": "Data/objects_baker_{{DifficultyToggle}}.json, Data/objects_barista_{{DifficultyToggle}}.json, Data/objects_boutique_{{DifficultyToggle}}.json, Data/objects_brewer_{{DifficultyToggle}}.json, Data/objects_confectioner_{{DifficultyToggle}}.json"
