            "When":{"Atelier.Cauldron/ConfigValue:FloristOnly": false}
        },

		{
            "LogName": "WAG Wild Food Objects and Rules",
            "Action": "Include",
            "FromFile": "Data/wildfood.json, Data/machines_forager.json",
