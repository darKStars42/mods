  "Changes": [
    // 0hearts - Eleanor's introduction
    // Location: your farm
    // Trigger: Entering your farm after seeing introduction event with Mr. Rasmodius.
    // Description: Eleanor introduces herself to the player, greeting them politely and explaining who she is and that she knows about you and Junimos.
    {
      "LogName": "Eleanor - event - 0hearts - Introduction",
      "Action": "EditData",
      "Target": "Data/Events/farm",
      "Entries": {
        "{{ModId}}_Intro/SawEvent 112":
        "none/-1000 -1000/farmer 2 2 2 {{ModId}} 3 3 0/
        skippable/
        addConversationTopic {{ModId}}.CT_Intro 2/
        pause 1111/
        speak {{ModId}} \"{{i18n:Events.Intro.0 |DT_name={{DT_name}} |DT_wizard_name={{DT_wizard_name}} |DT_wizard_fem={{DT_wizard_fem}} |default={{i18n:key_not_found}}}}\"/ 
        pause 555/
        emote farmer 40/
        pause 555/
        emote farmer 8/
        pause 555/
        speak {{ModId}} \"{{i18n:Events.Intro.1 |DT_name={{DT_name}} |DT_wizard_name={{DT_wizard_name}} |DT_wizard_fem={{DT_wizard_fem}} |default={{i18n:key_not_found}}}}\"/
