{
	"Format": "2.0.0",
	"ConfigSchema": {
		"Mayo Setting": {
			"AllowValues": "1, 5, 10, 15, 20",
			"Default": "5",
			"Description": "Adjusts how many eggs are needed to make quail mayo. (Recommended: 5)"
		},
		"Maturity Setting": {
			"AllowValues": "1, 2, 3, 4, 5",
			"Default": "3",
			"Description": "Adjusts how many days are required for chicks to mature into adults. (Recommended: 3)"
		},
		"Production Setting": {
			"AllowValues": "1, 2, 3",
			"Default": "1",
			"Description": "Adjusts how many days are required for quails produce eggs. (Recommended: 1)"
		},
		"Exclusion Setting": {
			"AllowValues": "false, true",
			"Default": "false",
			"Description": "Adjusts whether quail products are included or excluded from your shipping collection. Only enable if you wish to EXCLUDE quail products from your collection."
		},
		"Mayo or Custard": {
			"AllowValues": "Mayonnaise, Custard",
			"Default": "Mayonnaise",
			"Description": "Optionally renames quail mayo to custard."
		}
	},
